{
  "private": true,
  "type": "module",
  "name": "gql-semantic-nonnull-study",
  "scripts": {
    "prepare": "npm run generate",
    "clean": "rimraf -g .tsimp \"src/**/__generated__/**\" \"*.tsbuildinfo\"",
    "generate": "npm run generate:schema && npm run generate:prisma && npm run generate:graphql && npm run generate:relay",
    "generate:schema": "node --import=tsimp/import scripts/extractSchema.ts",
    "generate:prisma": "prisma generate",
    "generate:graphql": "npm run codegen",
    "generate:relay": "npm run relay",
    "prestart": "tsc -p tsconfig.build.json",
    "start:server": "cross-env DATABASE_NAME=dev node --import=tsimp/import src/server.ts",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\" \"npm run dev:relay\"",
    "dev:client": "vite",
    "dev:relay": "npm run relay -- --watch",
    "dev:server": "cross-env DATABASE_NAME=dev node --watch --import=tsimp/import src/server.ts",
    "migrate:dev": "cross-env DATABASE_NAME=dev prisma migrate dev",
    "migrate:dev:reset": "cross-env DATABASE_NAME=dev prisma migrate reset",
    "migrate:test": "cross-env DATABASE_NAME=test prisma migrate dev",
    "migrate:test:reset": "cross-env DATABASE_NAME=test prisma migrate reset",
    "seed": "cross-env DATABASE_NAME=dev node --import=tsimp/import scripts/seed.ts",
    "test": "cross-env DATABASE_NAME=test NODE_OPTIONS=--experimental-vm-modules jest",
    "compile": "tsc --noEmit",
    "format": "prettier . --write",
    "codegen": "graphql-codegen-esm --config codegen.yml",
    "relay": "node node_modules/relay-compiler/cli.js"
  },
  "author": "Quramy",
  "license": "MIT",
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/typescript": "4.0.9",
    "@graphql-codegen/typescript-resolvers": "4.2.1",
    "@ngneat/falso": "^7.0.0",
    "@quramy/jest-prisma-node": "^1.3.0",
    "@quramy/prisma-fabbrica": "^2.0.0",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.3.6",
    "@types/react-dom": "^18.3.0",
    "@types/react-relay": "^16.0.6",
    "@types/relay-runtime": "^17.0.4",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "jest": "^29.2.2",
    "jest-environment-node": "^29.2.2",
    "prettier": "^2.8.0",
    "prisma": "^5.0.0",
    "relay-compiler": "^18.0.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.0.3",
    "tsimp": "^2.0.11",
    "typescript": "~5.5.0",
    "vite": "^5.4.8",
    "vite-plugin-relay": "^2.1.0"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "dataloader": "^2.1.0",
    "graphql": "^16.9.0",
    "graphql-yoga": "^5.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-relay": "^18.0.0",
    "relay-runtime": "^18.0.0"
  }
}
